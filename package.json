{
  "name": "sls-plugin-additional-stacks",
  "version": "3.8.0",
  "description": "Serverless Framework Plugin to Deploy and Remove AWS Additional Stacks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "start": "npm link && nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/93v/sls-plugin-additional-stacks.git"
  },
  "keywords": [
    "serverless",
    "serverless-plugin",
    "sls",
    "sls-plugin",
    "aws",
    "additional",
    "stacks",
    "additional-stacks",
    "CloudFormation"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/sls-plugin-additional-stacks/issues"
  },
  "homepage": "https://github.com/93v/sls-plugin-additional-stacks#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/serverless": "^1.18.2",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "serverless": "^1.48.4",
    "ts-optchain": "^0.1.7"
  }
}
